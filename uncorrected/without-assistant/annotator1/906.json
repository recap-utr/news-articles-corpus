    "c17a8250-c60d-11ee-bbb2-f78c6e9d2422": {
      "atom": {
        "text": " Right now it looks like a two-speed Europe will be part of this vision",
        "reference": {
          "resource": "906",
          "offset": 1554,
          "text": " Right now it looks like a two-speed Europe will be part of this vision"
        }
      },
