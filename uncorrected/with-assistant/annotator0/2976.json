    "a1d19ab0-c77a-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Yulia Samoilova, who will represent Russia at the Eurovision-2017 song contest in Ukraine dismissed as \"laughable\" the Ukrainian authorities' threats not to allow her cross the country's border",
        "reference": {
          "resource": "2976",
          "text": "Yulia Samoilova, who will represent Russia at the Eurovision-2017 song contest in Ukraine dismissed as \"laughable\" the Ukrainian authorities' threats not to allow her cross the country's border"
        }
      },
      "metadata": {
        "created": "2024-02-09T18:40:00.987Z",
        "updated": "2024-02-09T18:40:00.987Z"
