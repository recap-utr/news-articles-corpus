          "config": {
            "model": "gpt-4-turbo-preview",
            "baseURL": "https://api.openai.com/v1"
          },
          "explanation": "Highlights the casualties and injuries from the attack."
        },
        "arguemapper": {
          "position": {
            "x": 362.00086051015245,
            "y": 326
          }
        }
      }
    },
    "fad563c0-ca05-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "There was no immediate claim of responsibility for the attack",
        "reference": {
          "resource": "646",
          "offset": 309,
          "text": "There was no immediate claim of responsibility for the attack"
        }
      },
      "metadata": {
        "created": "2024-02-13T00:22:32.700Z",
        "updated": "2024-02-13T00:22:32.700Z"
      },
      "userdata": {
        "assistant": {
          "config": {
            "model": "gpt-4-turbo-preview",
