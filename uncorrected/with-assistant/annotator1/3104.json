    "cef342a0-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "A US diplomat has been expelled from New Zealand after the embassy refused to waive his immunity during a police investigation",
        "reference": {
          "resource": "3104",
          "offset": 0,
          "text": "A US diplomat has been expelled from New Zealand after the embassy refused to waive his immunity during a police investigation"
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
        "assistant": {
          "config": {
            "model": "gpt-4-turbo-preview",
            "baseURL": "https://api.openai.com/v1"
          },
          "explanation": "States the main incident and action that triggered further discussion."
        },
        "arguemapper": {
          "position": {
            "x": 362.0000245055155,
            "y": 486
          }
        }
      }
    },
    "cef342a1-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "The staffer was allegedly involved in an incident on 12 March, but police were unable to question him after the embassy declined their request",
        "reference": {
          "resource": "3104",
          "offset": 128,
          "text": "The staffer was allegedly involved in an incident on 12 March, but police were unable to question him after the embassy declined their request"
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
        "assistant": {
          "config": {
            "model": "gpt-4-turbo-preview",
            "baseURL": "https://api.openai.com/v1"
          },
          "explanation": "Provides details on the reason behind the police's interest to question the diplomat and the obstruction faced."
        },
        "arguemapper": {
          "position": {
            "x": 712.0003853238984,
            "y": 752
          }
        }
      }
    },
    "cef342a2-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "New Zealand then asked the US to remove the man, who American officials confirmed had left on Saturday",
        "reference": {
          "resource": "3104",
          "offset": 272,
          "text": "New Zealand then asked the US to remove the man, who American officials confirmed had left on Saturday"
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
        "assistant": {
          "config": {
            "model": "gpt-4-turbo-preview",
            "baseURL": "https://api.openai.com/v1"
          },
          "explanation": "Describes New Zealand's response to the situation and its outcome."
        },
        "arguemapper": {
          "position": {
            "x": 362.00052944766145,
            "y": 752
          }
        }
      }
    },
    "cef342a3-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "Police have refused to give further details of the allegations",
        "reference": {
          "resource": "3104",
          "offset": 376,
          "text": "Police have refused to give further details of the allegations"
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
