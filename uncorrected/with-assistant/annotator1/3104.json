    "cef342a5-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "All diplomatic staff working in New Zealand are immune from prosecution, thanks to the 1961 Vienna Convention",
        "reference": {
          "resource": "3104",
          "offset": 675,
          "text": "All diplomatic staff working in New Zealand are immune from prosecution, thanks to the 1961 Vienna Convention"
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
        "assistant": {
          "config": {
            "model": "gpt-4-turbo-preview",
            "baseURL": "https://api.openai.com/v1"
          },
          "explanation": "Provides background on why the diplomat could not be legally pursued in New Zealand."
        },
        "arguemapper": {
          "position": {
            "x": 332.0006318625623,
            "y": 12
          }
        }
      }
    },
    "cef342a6-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "New Zealand's foreign ministry said it makes it \"clear with all diplomatic missions in New Zealand that it expects foreign diplomats to abide by New Zealand law, and to waive immunity... if there are allegations of serious crimes\"",
        "reference": {
          "resource": "3104",
          "offset": 795,
          "text": "New Zealand's foreign ministry said it makes it \"clear with all diplomatic missions in New Zealand that it expects foreign diplomats to abide by New Zealand law, and to waive immunity... if there are allegations of serious crimes\""
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
        "assistant": {
          "config": {
            "model": "gpt-4-turbo-preview",
            "baseURL": "https://api.openai.com/v1"
          },
          "explanation": "Illustrates New Zealand's stance on diplomatic immunity and its expectations from foreign diplomats in cases of serious crimes."
        },
        "arguemapper": {
          "position": {
            "x": 332.00004209770583,
            "y": 278
          }
        }
      }
