        "arguemapper": {
          "position": {
            "x": 12.000453746863185,
            "y": 12
          }
        }
      }
    },
    "cef342a4-ca4f-11ee-b5b8-bbee0a74ec6f": {
      "atom": {
        "text": "Police did say they were continuing to investigate the incident, which took place just outside Wellington",
        "reference": {
          "resource": "3104",
          "offset": 568,
          "text": "Police did say they were continuing to investigate the incident, which took place just outside Wellington"
        }
      },
      "metadata": {
        "created": "2024-02-13T09:11:01.834Z",
        "updated": "2024-02-13T09:11:01.834Z"
      },
      "userdata": {
