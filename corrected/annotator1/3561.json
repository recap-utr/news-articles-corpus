    "90af0fb6-c77f-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Norvik Banka, Latvia's bank number seven in terms of assets, has announced a range of steps to cut down its presence in the Russian banking sector",
        "reference": {
          "resource": "3561",
          "offset": 1825,
          "text": "Norvik Banka, Latvia's bank number seven in terms of assets, has announced a range of steps to cut down its presence in the Russian banking sector"
        }
      },
      "metadata": {
        "created": "2024-02-09T19:15:19.723Z",
