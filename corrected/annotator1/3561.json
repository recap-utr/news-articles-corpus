    "90af0fb0-c77f-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Russia's largest bank, Sberbank, has signed an agreement on selling 100% shares in its Ukrainian affiliation to a consortium that comprises a Latvian bank and a Belarusian company",
        "reference": {
          "resource": "3561",
          "offset": 26,
          "text": "Russia's largest bank, Sberbank, has signed an agreement on selling 100% shares in its Ukrainian affiliation to a consortium that comprises a Latvian bank and a Belarusian company"
        }
      },
      "metadata": {
        "created": "2024-02-09T19:15:19.723Z",
        "updated": "2024-02-09T19:15:19.723Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 362.0006251712276,
            "y": 414
          }
        }
      }
    },
    "90af0fb1-c77f-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "The closure of the transaction is expected before July 2017 after its endorsement by financial and antitrust regulators",
        "reference": {
          "resource": "3561",
          "offset": 528,
          "text": "The closure of the transaction is expected before July 2017 after its endorsement by financial and antitrust regulators"
        }
      },
      "metadata": {
        "created": "2024-02-09T19:15:19.723Z",
        "updated": "2024-02-09T19:15:19.723Z"
