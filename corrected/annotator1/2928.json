    "cf4686a0-c779-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Protesters have voiced their anger at rising anti-immigration sentiment in the UK in a series of demonstrations across the country",
        "reference": {
          "resource": "2928",
          "offset": 0,
          "text": "Protesters have voiced their anger at rising anti-immigration sentiment in the UK in a series of demonstrations across the country"
        }
      },
      "metadata": {
        "created": "2024-02-09T18:34:07.754Z",
        "updated": "2024-02-09T18:34:07.754Z"
