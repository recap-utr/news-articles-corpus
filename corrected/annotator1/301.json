    "ca330880-c41a-11ee-a06c-cb1cbb90ffa4": {
      "atom": {
        "text": "the group with the most headers was three times more likely to have symptoms --dizziness, feeling dazed, stopping play for medical attention or even losing consciousness --than those who rarely used their heads while playing",
        "reference": {
          "resource": "301",
          "offset": 795,
          "text": "The study, published in the February 1 edition of Neurology, found that the group with the most headers was three times more likely to have symptoms --dizziness, feeling dazed, stopping play for medical attention or even losing consciousness --than those who rarely used their heads while playing"
        }
      },
      "metadata": {
        "created": "2024-02-05T11:36:23.560Z",
        "updated": "2024-02-05T11:36:23.560Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 232.71100921948027,
            "y": 406.53348430628785
          }
        }
      }
    },
    "0886ff10-c41b-11ee-a06c-cb1cbb90ffa4": {
      "atom": {
        "text": "Our study suggests that you don't need an overt collision to warrant this type of concern",
        "reference": {
          "resource": "301",
          "offset": 1333,
          "text": "Our study suggests that you don't need an overt collision to warrant this type of concern"
        }
      },
      "metadata": {
