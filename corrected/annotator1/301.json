    "a24ec890-c41a-11ee-a06c-cb1cbb90ffa4": {
      "atom": {
        "text": "injuries such as concussion resulted mainly from full-on head impacts with other players or collisions with the goal posts",
        "reference": {
          "resource": "301",
          "offset": 304,
          "text": "previous studies that suggested that injuries such as concussion resulted mainly from full-on head impacts with other players or collisions with the goal posts"
        }
      },
