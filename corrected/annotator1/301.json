    "8bfecef0-c41a-11ee-a06c-cb1cbb90ffa4": {
      "atom": {
        "text": "Soccer players who-regularly header a ball are three times more likely to develop the symptoms of concussions compared to those that don't",
        "reference": {
          "resource": "301",
          "offset": 0,
          "text": "Soccer players who-regularly header a ball are three times more likely to develop the symptoms of concussions compared to those that don't"
        }
      },
