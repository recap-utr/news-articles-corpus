    "26d70190-c41b-11ee-a06c-cb1cbb90ffa4": {
      "atom": {
        "text": "single or repeated concussions do lead to neurological problems later in life",
        "reference": {
          "resource": "301",
          "offset": 1574,
          "text": "single or repeated concussions do lead to neurological problems later in life"
        }
      },
      "metadata": {
        "created": "2024-02-05T11:38:58.985Z",
        "updated": "2024-02-05T11:38:58.985Z"
