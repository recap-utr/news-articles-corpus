    "74cd9902-c77b-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Chuck Berry was rock's greatest practitioner, guitarist, and the greatest pure rock 'n' roll writer who ever lived",
        "reference": {
          "resource": "2997",
          "offset": 456,
          "text": "Chuck Berry was rock's greatest practitioner, guitarist, and the greatest pure rock 'n' roll writer who ever lived"
        }
