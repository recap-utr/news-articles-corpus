    "74cd9900-c77b-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Bruce Springsteen, Ringo Starr, Questlove, Slash, Mick Jagger and others remembered Berry for his guitar style, his lyrics, and his inspiration",
        "reference": {
          "resource": "2997",
          "offset": 162,
          "text": "Bruce Springsteen, Ringo Starr, Questlove, Slash, Mick Jagger and others remembered Berry for his guitar style, his lyrics, and his inspiration"
