    "74cd9901-c77b-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "I am so sad to hear of Chuck Berry's passing. I want to thank him for all the inspirational music he gave to us",
        "reference": {
          "resource": "2997",
          "offset": 310,
          "text": "I am so sad to hear of Chuck Berry's passing. I want to thank him for all the inspirational music he gave to us"
        }
