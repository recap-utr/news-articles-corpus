    "0daa12c8-c768-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "He added that Kim had a history of heart problems and had been treated in hospital in the past",
        "reference": {
          "resource": "1959",
          "offset": 1802,
          "text": "He added that Kim had a history of heart problems and had been treated in hospital in the past"
        }
      },
      "metadata": {
        "created": "2024-02-09T16:27:01.484Z",
        "updated": "2024-02-09T16:27:01.484Z"
