    "0daa12cb-c768-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "the only suspects charged in the case so far are an Indonesian woman and a Vietnamese woman. They face the death penalty if convicted",
        "reference": {
          "resource": "1959",
          "text": "the only suspects charged in the case so far are an Indonesian woman and a Vietnamese woman. They face the death penalty if convicted"
        }
      },
      "metadata": {
        "created": "2024-02-09T16:27:01.484Z",
        "updated": "2024-02-09T16:27:01.484Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 1412.0006126991366,
            "y": 664
