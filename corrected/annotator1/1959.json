    "0daa12ca-c768-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "South Korean intelligence and US officials say the murder was an assassination organised by North Korean agents",
        "reference": {
          "resource": "1959",
          "offset": 2443,
          "text": "South Korean intelligence and US officials say the murder was an assassination organised by North Korean agents"
        }
      },
      "metadata": {
        "created": "2024-02-09T16:27:01.484Z",
        "updated": "2024-02-09T16:27:01.484Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 2162.0008908658406,
            "y": 278
          }
        }
