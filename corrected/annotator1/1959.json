    "0daa12cd-c768-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "questions remain about who else may have been involved with the women",
        "reference": {
          "resource": "1959",
          "offset": 2889,
          "text": "questions remain about who else may have been involved with the women"
        }
      },
