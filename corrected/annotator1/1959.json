    "0daa12cc-c768-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "They have told diplomats from their home countries that they thought they were participating in a prank for reality television",
        "reference": {
          "resource": "1959",
          "offset": 2699,
          "text": "They have told diplomats from their home countries that they thought they were participating in a prank for reality television"
        }
      },
      "metadata": {
        "created": "2024-02-09T16:27:01.484Z",
        "updated": "2024-02-09T16:27:01.484Z"
