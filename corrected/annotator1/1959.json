    "0daa12c9-c768-11ee-a75d-3d21b0b510a2": {
      "atom": {
        "text": "Relations between Malaysia and North Korea, who have maintained friendly ties for decades, have soured since the killing of Kim Jong-nam",
        "reference": {
          "resource": "1959",
          "offset": 2220,
          "text": "Relations between Malaysia and North Korea, who have maintained friendly ties for decades, have soured since the killing of Kim Jong-nam"
        }
      },
      "metadata": {
        "created": "2024-02-09T16:27:01.484Z",
        "updated": "2024-02-09T16:27:01.484Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
