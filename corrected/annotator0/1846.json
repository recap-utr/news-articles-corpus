    "617f9326-ca4c-11ee-a4d8-23b63abe8329": {
      "atom": {
        "text": "FARC leader Rodrigo \"Timochenko\" Londono also tweeted that the leftist rebels were laying down their guns \"with enthusiasm\"",
        "reference": {
          "resource": "1846",
          "offset": 1459,
          "text": "FARC leader Rodrigo \"Timochenko\" Londono also tweeted that the leftist rebels were laying down their guns \"with enthusiasm\""
        }
      },
      "metadata": {
        "created": "2024-02-13T08:46:29.714Z",
        "updated": "2024-02-13T08:46:29.714Z"
      },
      "userdata": {
