    "617f9328-ca4c-11ee-a4d8-23b63abe8329": {
      "atom": {
        "text": "It is currently engaged in rocky peace talks with the government",
        "reference": {
          "resource": "1846",
          "offset": 1725,
          "text": "It is currently engaged in rocky peace talks with the government"
        }
      },
