    "617f9327-ca4c-11ee-a4d8-23b63abe8329": {
      "atom": {
        "text": "The FARC's disarmament will leave rival movement ELN as the only rebel group still waging a multi-sided war that has killed 260,000 people",
        "reference": {
          "resource": "1846",
          "offset": 1585,
          "text": "The FARC's disarmament will leave rival movement ELN as the only rebel group still waging a multi-sided war that has killed 260,000 people"
        }
      },
      "metadata": {
        "created": "2024-02-13T08:46:29.714Z",
        "updated": "2024-02-13T08:46:29.714Z"
