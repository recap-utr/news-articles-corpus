    "53ecfd96-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "The advances come days after Iraqi forces officially launched the operation to push ISIL out of Mosul's western half",
        "reference": {
          "resource": "1378",
          "offset": 1158,
          "text": "The advances come days after Iraqi forces officially launched the operation to push ISIL out of Mosul's western half"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 1812.000403355522,
            "y": 302
          }
        }
      }
    },
    "53ecfd97-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "The operation to retake Iraq's second largest city was officially launched in October and in January its eastern half was declared \"fully liberated.\"",
        "reference": {
          "resource": "1378",
          "offset": 1276,
          "text": "The operation to retake Iraq's second largest city was officially launched in October and in January its eastern half was declared \"fully liberated.\""
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 1812.0002104202035,
            "y": 592
          }
        }
      }
