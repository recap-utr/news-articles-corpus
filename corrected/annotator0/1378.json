    "53ecfd91-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "Counterterrorism service (CTS) troops and elite interior ministry units known as Rapid Response forces descended on the airport early on Thursday and the nearby Ghazlani military complex",
        "reference": {
          "resource": "1378",
          "offset": 165,
          "text": "Counterterrorism service (CTS) troops and elite interior ministry units known as Rapid Response forces descended on the airport early on Thursday and the nearby Ghazlani military complex"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
