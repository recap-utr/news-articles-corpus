    "53ecfd98-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "A US-led coalition has been providing close air support throughout the campaign to retake Iraq's second-largest city",
        "reference": {
          "resource": "1378",
          "offset": 1426,
          "text": "A US-led coalition has been providing close air support throughout the campaign to retake Iraq's second-largest city"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 2162.000409555947,
            "y": 302
          }
        }
      }
    },
    "53ecfd99-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "US special operations forces are embedded with some Iraqi units and thousands of US troops are in Iraq providing logistical and other support",
        "reference": {
          "resource": "1378",
          "text": "US special operations forces are embedded with some Iraqi units and thousands of US troops are in Iraq providing logistical and other support"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
