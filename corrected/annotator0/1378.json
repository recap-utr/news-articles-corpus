    "53ecfd9a-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "US Secretary of Defence Jim Mattis was holding discussions with US and Iraqi officials, a week before he is expected to present a new strategy to President Donald Trump for defeating ISIL",
        "reference": {
          "resource": "1378",
          "text": "US Secretary of Defence Jim Mattis was holding discussions with US and Iraqi officials, a week before he is expected to present a new strategy to President Donald Trump for defeating ISIL"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 2162.0003873948126,
            "y": 906
          }
        }
      }
    },
    "53ecfd9b-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "Trump has repeatedly vowed to eliminate the group but has provided few details about how his approach might differ from that of the Obama administration",
        "reference": {
          "resource": "1378",
          "text": "Trump has repeatedly vowed to eliminate the group but has provided few details about how his approach might differ from that of the Obama administration"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 2162.000898109582,
            "y": 1220
          }
        }
      }
    },
    "53ecfd9c-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "The battle for western Mosul, the group's last major urban bastion in Iraq, is expected to be the most daunting yet",
        "reference": {
          "resource": "1378",
          "text": "The battle for western Mosul, the group's last major urban bastion in Iraq, is expected to be the most daunting yet"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
        "arguemapper": {
          "position": {
            "x": 362.0002125308244,
            "y": 314
          }
        }
      }
    },
    "53ecfd9d-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "The presence of up to 750,000 civilians also poses a challenge",
        "reference": {
          "resource": "1378",
          "text": "The presence of up to 750,000 civilians also poses a challenge"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
