    "53ecfd90-ca0d-11ee-ab2a-4dc8d1f5b568": {
      "atom": {
        "text": "US-backed Iraqi security forces closing in on the ISIL-held western half of Mosul have stormed the city's airport and a nearby military base",
        "reference": {
          "resource": "1378",
          "offset": 0,
          "text": "US-backed Iraqi security forces closing in on the ISIL-held western half of Mosul have stormed the city's airport and a nearby military base"
        }
      },
      "metadata": {
        "created": "2024-02-13T01:15:08.649Z",
        "updated": "2024-02-13T01:15:08.649Z"
      },
      "userdata": {
